{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "guzzlehttp/guzzle": "~6.0",
        "aws/aws-sdk-php": "~3.0",
        "predis/predis": "~1.1@dev",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-rackspace": "~1.0",
        "league/flysystem": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "laravelcollective/html": "5.5.*",
        "filp/whoops": "^2.1",
        "intervention/image": "^2.3",
        "nwidart/laravel-modules": "^2.0",
        "barryvdh/laravel-debugbar": "^2.2",
        "laravie/html": "5.5.*",
        "orchestra/asset": "3.5.*",
        "doctrine/dbal": "^2.5",
        "maatwebsite/excel": "~2.1.0",
        "twig/twig": "~1.0",
        "barryvdh/laravel-dompdf": "^0.8",
        "yajra/laravel-datatables-oracle": "8.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "files": [
            "app/Helpers/User.php",
            "app/Helpers/Geo.php",
            "app/Helpers/Setting.php",
            "app/Helpers/View.php",
            "app/Helpers/Debug.php",
            "app/Helpers/Appraisal.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Admin\\": "modules/Admin/",
            "Dashboard\\": "modules/Dashboard/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "composer admin-publish",
            "composer public-publish",
            "php artisan migrate"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "composer admin-publish",
            "composer public-publish",
            "php artisan migrate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "admin-publish": "php artisan module:publish Admin",
        "public-publish": "php artisan vendor:publish --tag=public --force"
    },
    "config": {
        "preferred-install": "dist"
    }
}
